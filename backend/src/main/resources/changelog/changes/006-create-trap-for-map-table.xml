<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.9.xsd">

    <changeSet id="006-1" author="AnfiNa">
        <createTable tableName="trap_for_map">
            <column name="id" type="INT" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="id_map" type="INT">
                <constraints nullable="false" foreignKeyName="fk_trap_for_map_map" references="maps(id)"/>
            </column>
            <column name="id_traps" type="INT">
                <constraints nullable="false" foreignKeyName="fk_trap_for_map_traps" references="traps(id)"/>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>