<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.9.xsd">

    <!-- Создание таблицы pictures -->
    <changeSet id="004-1" author="dev">
        <createTable tableName="pictures">
            <column name="id" type="INT" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="trap_id" type="INT">
                <constraints nullable="false" foreignKeyName="fk_pictures_trap" references="traps(id)"/>
            </column>
            <column name="picture" type="TEXT">
                <constraints nullable="false" unique="true"/>
            </column>
            <column name="number" type="INT">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>