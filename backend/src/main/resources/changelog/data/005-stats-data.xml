<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.9.xsd">

    <changeSet id="005-2" author="dev">
        <loadData tableName="stats" file="changelog/data/stats.csv">
            <column name="id" type="NUMERIC"/>
            <column name="id_user" type="NUMERIC"/>
            <column name="id_map" type="NUMERIC"/>
            <column name="time" type="NUMERIC"/>
            <column name="role" type="BOOLEAN"/>
        </loadData>
    </changeSet>
    
</databaseChangeLog>